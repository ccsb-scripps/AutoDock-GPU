npts 60 60 60                      # num.grid points in xyz
gridfld protein.maps.fld              # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C OA N SA HD NA       # receptor atom types
ligand_types A C Cl I NA F OA N P S Br SA HD # ligand atom types
receptor protein.pdbqt                # macromolecule
gridcenter -3 0 0       # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map protein.A.map  # atom-specific affinity map
map protein.C.map  # atom-specific affinity map
map protein.Cl.map  # atom-specific affinity map
map protein.I.map  # atom-specific affinity map
map protein.NA.map  # atom-specific affinity map
map protein.F.map  # atom-specific affinity map
map protein.OA.map  # atom-specific affinity map
map protein.N.map  # atom-specific affinity map
map protein.P.map  # atom-specific affinity map
map protein.S.map  # atom-specific affinity map
map protein.Br.map  # atom-specific affinity map
map protein.SA.map  # atom-specific affinity map
map protein.HD.map  # atom-specific affinity map
elecmap protein.e.map                 # electrostatic potential map
dsolvmap protein.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
